= form_for @emplproj do |f|
  - if @emplproj.errors.any?
    #error_explanation
      %h2= "#{pluralize(@emplproj.errors.count, "error")} prohibited this emplproj from being saved:"
      %ul
        - @emplproj.errors.full_messages.each do |message|
          %li= message

  .field
    = f.label :empl
    = f.text_field :empl
  .field
    = f.label :project
    = f.text_field :project
  .actions
    = f.submit 'Save'
